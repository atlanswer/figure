{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/pyodide/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, immutable, max-age=604800, s-maxage=86400"
        }
      ]
    }
  ]
}
